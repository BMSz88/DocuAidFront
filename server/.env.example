# Server Configuration
PORT=3001

# MongoDB Connection
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority

# Authentication Secrets
SESSION_SECRET=your_session_secret_here
